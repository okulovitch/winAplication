<Window x:Class="kursachMain.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kursachMain"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="#FF631717" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>

    <Grid Margin="207,90,184,90">

        <Border Background="#FF895541" BorderBrush="Gainsboro" BorderThickness="1" Margin="-69,0,-35,-59">
            <StackPanel Margin="10" Cursor="ScrollW">
                <Button x:Name="btn_PracticMain" Click="Button_Click">Просмотр практики</Button>
                <Button x:Name="btn_StudentsMain" Margin="0,10">Студенты</Button>
                <Button x:Name="btn_PrepodsMain" Click="Button_Click_1" >Преподаватели</Button>
                <Button  Margin="0,10">Распределить на практику</Button>
                <Button  Margin="0" Content="Предприятия" Click="Button_Click_2"/>


            </StackPanel>

        </Border>

    </Grid>
</Window>
